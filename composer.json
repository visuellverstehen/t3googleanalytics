{
    "name": "visuellverstehen/t3googleanalytics",
    "type": "typo3-cms-extension",
    "description": "An easy and basic Google Analytics extension for TYPO3.",
    "license": "MIT",
    "authors": [
        {
            "name": "visuellverstehen",
            "email": "kontakt@visuellverstehen.de",
            "homepage": "https://www.visuellverstehen.de"
        }
    ],
    "require": {
        "typo3/cms-core": "7.6 - 11.5",
        "php": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "VV\\T3googleanalytics\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "t3googleanalytics"
        }
    }
}
